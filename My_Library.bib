@article{pulestonPolyamineMetabolismCentral2021,
  title = {Polyamine Metabolism Is a Central Determinant of Helper {{T}}~Cell Lineage Fidelity},
  author = {Puleston, Daniel J. and Baixauli, Francesc and Sanin, David E. and Edwards-Hicks, Joy and Villa, Matteo and Kabat, Agnieszka M. and Kami\'nski, Marcin M. and Stanckzak, Michal and Weiss, Hauke J. and Grzes, Katarzyna M. and Piletic, Klara and Field, Cameron S. and Corrado, Mauro and Haessler, Fabian and Wang, Chao and Musa, Yaarub and Schimmelpfennig, Lena and Flachsmann, Lea and Mittler, Gerhard and Yosef, Nir and Kuchroo, Vijay K. and Buescher, Joerg M. and Balabanov, Stefan and Pearce, Edward J. and Green, Douglas R. and Pearce, Erika L.},
