  author = {Tsch\"ope, Carsten and Ammirati, Enrico and Bozkurt, Biykem and Caforio, Alida L. P. and Cooper, Leslie T. and Felix, Stephan B. and Hare, Joshua M. and Heidecker, Bettina and Heymans, Stephane and H\"ubner, Norbert and Kelle, Sebastian and Klingel, Karin and Maatz, Henrike and Parwani, Abdul S. and Spillmann, Frank and Starling, Randall C. and Tsutsui, Hiroyuki and Seferovic, Petar and Van Linthout, Sophie},
  date = {2021-03},
  journaltitle = {Nature Reviews Cardiology},
