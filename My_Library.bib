@article{henikoffEfficientChromatinAccessibility2020,
  title = {Efficient Chromatin Accessibility Mapping in Situ by Nucleosome-Tethered Tagmentation},
  author = {Henikoff, Steven and Henikoff, Jorja G and Kaya-Okur, Hatice S and Ahmad, Kami},
