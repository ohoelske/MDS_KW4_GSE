  langid = {english},
  annotation = {Bandiera\_abtest: a Cg\_type: Nature Research Journals Primary\_atype: Research Subject\_term: Endoplasmic reticulum;Heart failure;Stress signalling Subject\_term\_id: endoplasmic-reticulum;heart-failure;stress-signalling},
