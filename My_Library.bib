  keywords = {antiretroviral,endogenous retrovirus,high fat diet,keratinocytes,microbiota,skin immunity,Staphylococcus epidermidis,STING,T cells,tissue repair},
  file = {C\:\\Users\\Oliver\\Zotero\\storage\\B75KV2SJ\\Lima-Junior et al. - 2021 - Endogenous retroviruses promote homeostatic and in.pdf;C\:\\Users\\Oliver\\Zotero\\storage\\IA22ZDAD\\S0092867421006504.html}
