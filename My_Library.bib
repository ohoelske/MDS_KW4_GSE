@article{elsaidWaveBipotentILCrestricted2021,
  title = {A Wave of Bipotent {{T}}/{{ILC-restricted}} Progenitors Shapes the Embryonic Thymus Microenvironment in a Time-Dependent Manner},
  author = {Elsaid, Ramy and Meunier, Sylvain and Burlen-Defranoux, Odile and Soares-da-Silva, Francisca and Perchet, Thibaut and Iturri, Lorea and Freyer, Laina and Vieira, Paulo and Pereira, Pablo and Golub, Rachel and Bandeira, Antonio and Perdiguero, Elisa Gomez and Cumano, Ana},
  date = {2021-02-25},
  journaltitle = {Blood},
