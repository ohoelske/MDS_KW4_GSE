@article{schnellStemlikeIntestinalTh172021,
  title = {Stem-like Intestinal {{Th17}} Cells Give Rise to Pathogenic Effector {{T}} Cells during Autoimmunity},
  author = {Schnell, Alexandra and Huang, Linglin and Singer, Meromit and Singaraju, Anvita and Barilla, Rocky M. and Regan, Brianna M.L. and Bollhagen, Alina and Thakore, Pratiksha I. and Dionne, Danielle and Delorey, Toni M. and Pawlak, Mathias and Meyer zu Horste, Gerd and Rozenblatt-Rosen, Orit and Irizarry, Rafael A. and Regev, Aviv and Kuchroo, Vijay K.},
