@article{biccardCardiacBiomarkersPrediction2014,
  title = {Cardiac Biomarkers in the Prediction of Risk in the Non-Cardiac Surgery Setting},
  author = {Biccard, B. M. and Devereaux, P. J. and Rodseth, R. N.},
  date = {2014},
  journaltitle = {Anaesthesia},
  volume = {69},
