@article{drokhlyanskyHumanMouseEnteric2020,
  title = {The {{Human}} and {{Mouse Enteric Nervous System}} at {{Single-Cell Resolution}}},
  author = {Drokhlyansky, Eugene and Smillie, Christopher S. and Van Wittenberghe, Nicholas and Ericsson, Maria and Griffin, Gabriel K. and Eraslan, Gokcen and Dionne, Danielle and Cuoco, Michael S. and Goder-Reiser, Max N. and Sharova, Tatyana and Kuksenko, Olena and Aguirre, Andrew J. and Boland, Genevieve M. and Graham, Daniel and Rozenblatt-Rosen, Orit and Xavier, Ramnik J. and Regev, Aviv},
  date = {2020-09-17},
  journaltitle = {Cell},
  shortjournal = {Cell},
  volume = {182},
